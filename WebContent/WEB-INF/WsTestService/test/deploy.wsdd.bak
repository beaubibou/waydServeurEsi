<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from WsTestService WSDL service -->

  <service name="WsTest" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://test"/>
      <parameter name="wsdlServiceElement" value="WsTestService"/>
      <parameter name="schemaQualified" value="http://test"/>
      <parameter name="wsdlServicePort" value="WsTest"/>
      <parameter name="className" value="test.WsTestSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="WsTest"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="init" qname="operNS:init" xmlns:operNS="http://test" soapAction="" >
      </operation>
      <operation name="inscriptionNotation" qname="operNS:inscriptionNotation" xmlns:operNS="http://test" soapAction="" >
      </operation>
      <operation name="inscriptionAnnulation" qname="operNS:inscriptionAnnulation" xmlns:operNS="http://test" soapAction="" >
      </operation>
      <operation name="rechercheActivite" qname="operNS:rechercheActivite" xmlns:operNS="http://test" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="init inscriptionNotation rechercheActivite inscriptionAnnulation"/>

  </service>
</deployment>
